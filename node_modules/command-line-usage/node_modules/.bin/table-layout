#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/velo/node_modules/.pnpm/table-layout@3.0.2/node_modules/table-layout/bin/node_modules:/mnt/e/velo/node_modules/.pnpm/table-layout@3.0.2/node_modules/table-layout/node_modules:/mnt/e/velo/node_modules/.pnpm/table-layout@3.0.2/node_modules:/mnt/e/velo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/velo/node_modules/.pnpm/table-layout@3.0.2/node_modules/table-layout/bin/node_modules:/mnt/e/velo/node_modules/.pnpm/table-layout@3.0.2/node_modules/table-layout/node_modules:/mnt/e/velo/node_modules/.pnpm/table-layout@3.0.2/node_modules:/mnt/e/velo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../table-layout@3.0.2/node_modules/table-layout/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../table-layout@3.0.2/node_modules/table-layout/bin/cli.js" "$@"
fi
